# gcc manpages are non-free
g++-mingw-w64-i686: binary-without-manpage

# This isn't a spelling error
g++-mingw-w64-i686: spelling-error-in-binary usr/lib/gcc/i686-w64-mingw32/*/cc1plus tEH the

# Hardening isn't supported (yet)
g++-mingw-w64-i686: hardening-no-relro
g++-mingw-w64-i686: hardening-no-fortify-functions

# DLLs exported via Samba need to be executable (see #796223)
g++-mingw-w64-i686: executable-not-elf-or-script usr/lib/gcc/i686-w64-mingw32/*/*.dll
